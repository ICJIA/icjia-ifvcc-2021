{
  "name": "icjia-ifvcc-2021",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "serve": "npm run schema && vue-cli-service serve",
    "build": "node generateSearchIndex && npm run schema && npm run build:lambda && vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "build:lambda": "netlify-lambda build ./src/lambda",
    "build:modern": "npm run schema && npm run build:lambda && vue-cli-service build --modern",
    "build:serve": "npm run build && serve ./dist --single",
    "schema": "get-graphql-schema https://ifvcc.icjia-api.cloud/graphql -j > schema.json",
    "start:lambda": "netlify-lambda serve ./src/lambda",
    "release:major": "changelog -M && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version major && git push origin && git push origin --tags",
    "release:minor": "changelog -m && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version minor && git push origin && git push origin --tags",
    "release:patch": "changelog -p && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version patch && git push origin && git push origin --tags"
  },
