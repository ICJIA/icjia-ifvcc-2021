{
    "publicPath": "/",
    "clientURL": "https://icjiaifvcc.netlify.app",
    "namespace": "icjia-",
    "defaultAvatar": "https://cdn.vuetifyjs.com/images/john.jpg",
    "masonryBlockCols": 6,
    "api": {
        "base": "https://ifvcc.icjia-api.cloud",
        "baseGraphQL": "https://ifvcc.icjia-api.cloud/graphql",
        "baseClient": "https://ifvcc.icjia.cloud",
        "forgetPassword": "/auth/forgot-password",
        "resetPassword": "/auth/reset-password",
        "resetPasswordCallback": "/reset",
        "posts": "/posts",
        "login": "/auth/local",
        "timeout": 10000,
        "register": "/auth/local/register"
    },
    "home": {
        "eventLimit": 3,
        "postLimit": 3
    },
    "image": {
        "server": "https://image.icjia.cloud",
        "thumbHeight": 100,
        "thumbWidth": 0,
        "splashHeight": 400,
        "splashWidth": 0
    },
    "timezone": "America/Chicago",
    "underConstruction": false,
    "hoursToShowUpdated": 1,
    "pathMeta": {
        "tags": {
            "prefix": "/tags"
        },
        "events": {
            "prefix": "/events"
        },
        "pages": {
            "prefix": "/"
        },
        "posts": {
            "prefix": "/news"
        },
        "crest": {
            "prefix": "/crest"
        },
        "counties": {
            "prefix": "/counties"
        },
        "councils": {
            "prefix": "/circuits"
        }
    },
    "search": {
        "isCaseSensitive": false,
        "includeScore": true,
        "shouldSort": true,
        "includeMatches": true,
        "findAllMatches": true,
        "minMatchCharLength": 3,
        "location": 0,
        "threshold": 0.6,
        "distance": 100,
        "useExtendedSearch": false,
        "ignoreLocation": false,
        "ignoreFieldNorm": false,
        "keys": [
            {
                "name": "summary",
                "weight": 3
            },
            {
                "name": "title",
                "weight": 1
            },
            {
                "name": "name",
                "weight": 1
            },
            {
                "name": "headings",
                "weight": 3
            },
            {
                "name": "type",
                "weight": 2
            },
            {
                "name": "rawText",
                "weight": 1
            }
        ]
    }
}